{
  "name": "fitstravaapp",
  "version": "1.0.0",
  "description": "Fitbit To Strava Migration App",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:jquery": "cpx \"node_modules/jquery/dist/*\" lib/jquery --verbose",
    "postinstall:bootstrap": "cpx \"node_modules/bootstrap/dist/**/*\" lib/bootstrap --verbose",
    "postinstall:jqueryui": "cpx \"node_modules/jquery-ui/dist/**/*\" lib/jquery-ui --verbose",
    "postinstall:bootstrap-icons": "cpx \"node_modules/bootstrap-icons/**/*\" lib/bootstrap-icons --verbose",
    "postinstall:bootstrap-datepicker": "cpx \"node_modules/bootstrap-datepicker/dist/**/*\" lib/bootstrap-datepicker --verbose"
  },
  "directories": {
    "lib": "lib"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-datepicker": "^1.10.0",
    "bootstrap-icons": "^1.11.3",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5"
  }
}
